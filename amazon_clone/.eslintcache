n        tempProducts.push({\n          id: doc.id,\n          product: doc.data(),\n        });\n      });\n      setProducts(tempProducts);\n    });\n  }, []);\n\n  // console.log(products);\n\n  return (\n    <div className=\"home\">\n      <div className=\"home-container\">\n        <div className=\"home-banner\"></div>\n      </div>\n      <div className=\"home-content\">\n        <div className=\"home-row\">\n          {products.slice(0, 2).map((item) => (\n            <Product\n              id={item.id}\n              key={item.id}\n              title={item.product.title}\n              price={item.product.price}\n              rating={item.product.rating}\n              image={item.product.image}\n            />\n          ))}\n        </div>\n        <div className=\"home-row\">\n          {products.slice(2, 5).map((item) => (\n            <Product\n              id={item.id}\n              key={item.id}\n              title={item.product.title}\n              price={item.product.price}\n              rating={item.product.rating}\n              image={item.product.image}\n            />\n          ))}\n        </div>\n        <div className=\"home-row\">\n          <TopCardsLineOne />\n        </div>\n        <div className=\"home-row\">\n          <TopCardsLineTwo />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Home;\n","C:\\Users\\jhaka\\Desktop\\amazon_clone\\src\\CartTotal.js",[],"C:\\Users\\jhaka\\Desktop\\amazon_clone\\src\\CartItems.js",["72"],"import React from \"react\";\nimport \"./CartItems.css\";\nimport db from \"./firebase\";\n\nconst CartItems = (props) => {\n  let options = [];\n  for (let i = 1; i < 21; i++) {\n    options.push(\n      <option className=\"options\" value={i}>\n        Qty: {i}\n      </option>\n    );\n  }\n\n  const deleteItem = () => {\n    db.collection(\"cart-items\").doc(props.id).delete();\n  };\n\n  const changeQuantity = (val) => {\n    db.collection(\"cart-items\")\n      .doc(props.id)\n      .update({\n        quantity: parseInt(val),\n      });\n  };\n\n  return (\n    <div className=\"cartItem\">\n      <div className=\"cartItem-image\">\n        <img alt=\"image\" src={props.image} />\n      </div>\n      <div className=\"cartItem-info\">\n        <div className=\"cartItem-title\">{props.title}</div>\n        <div className=\"cartItem-actions\">\n          <div className=\"cartItem-quantity\">\n            <select\n              value={props.quantity}\n              onChange={(e) => changeQuantity(e.target.value)}\n            >\n              {options}\n            </select>\n          </div>\n          <div className=\"cartItem-delete\" onClick={deleteItem}>\n            Delete\n          </div>\n        </div>\n      </div>\n      <div className=\"cartItem-price\">\n        ₹{(props.price * props.quantity).toLocaleString(\"en-IN\")}\n      </div>\n    </div>\n  );\n};\n\nexport default CartItems;\n","C:\\Users\\jhaka\\Desktop\\amazon_clone\\src\\Product.js",["73"],"import React from \"react\";\nimport db from \"./firebase\";\nimport \"./Product.css\";\n\nfunction Product(props) {\n  const addToCart = () => {\n    let cartItem = db.collection(\"cart-items\").doc(props.id);\n    cartItem.get().then((doc) => {\n      if (doc.exists) {\n        cartItem.update({\n          quantity: parseInt(doc.data().quantity) + 1,\n        });\n      } else {\n        cartItem.set({\n          title: props.title,\n          price: props.price,\n          image: props.image,\n          quantity: 1,\n        });\n      }\n    });\n  };\n\n  // console.log(props);\n\n  return (\n    <div className=\"product\">\n      <div className=\"product-info\">\n        <span className=\"product-title\">{props.title}</span>\n        <span className=\"product-price\">\n          ₹{props.price.toLocaleString(\"en-IN\")}\n        </span>\n        <span className=\"product-rating\">\n          {Array(props.rating)\n            .fill()\n            .map((_, i) => (\n              <p>⭐</p>\n            ))}\n        </span>\n      </div>\n      <img alt=\"image\" src={props.image} />\n      <button onClick={addToCart}>Add to Cart</button>\n    </div>\n  );\n}\n\nexport default Product;\n","C:\\Users\\jhaka\\Desktop\\amazon_clone\\src\\TopCards.js",[],"C:\\Users\\jhaka\\Desktop\\amazon_clone\\src\\ProductOptions.js",[],"C:\\Users\\jhaka\\Desktop\\amazon_clone\\src\\FourImage.js",[],"C:\\Users\\jhaka\\Desktop\\amazon_clone\\src\\OneImage.js",["74"],"import React from \"react\";\n\nconst OneImage = (props) => {\n  return (\n    <div className=\"productOptions-img\">\n      <img src={props.img1} />\n    </div>\n  );\n};\n\nexport default OneImage;\n","C:\\Users\\jhaka\\Desktop\\amazon_clone\\src\\productsData.js",[],{"ruleId":"75","severity":1,"message":"76","line":16,"column":31,"nodeType":"77","messageId":"78","endLine":16,"endColumn":33},{"ruleId":"79","severity":1,"message":"80","line":20,"column":11,"nodeType":"81","endLine":23,"endColumn":13},{"ruleId":"75","severity":1,"message":"76","line":13,"column":31,"nodeType":"77","messageId":"78","endLine":13,"endColumn":33},{"ruleId":"79","severity":1,"message":"80","line":30,"column":9,"nodeType":"81","endLine":30,"endColumn":46},{"ruleId":"79","severity":1,"message":"80","line":41,"column":7,"nodeType":"81","endLine":41,"endColumn":44},{"ruleId":"82","severity":1,"message":"83","line":6,"column":7,"nodeType":"81","endLine":6,"endColumn":31},"array-callback-return","Array.prototype.map() expects a return value from arrow function.","ArrowFunctionExpression","expectedInside","jsx-a11y/img-redundant-alt","Redundant alt attribute. Screen-readers already announce `img` tags as an image. You don’t need to use the words `image`, `photo,` or `picture` (or any specified custom words) in the alt prop.","JSXOpeningElement","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images."]